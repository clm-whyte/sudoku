{
  "name": "sudoku",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "jest -- --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "prettier --write . && eslint --quiet --fix .",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "chromatic": "chromatic --project-token=2q214bba2a7",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --spec cypress/integration/sudoku.spec.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "fontsource-roboto": "^2.1.4",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "source-map-explorer": "^2.4.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-storyshots": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.2",
    "@storybook/react": "^5.3.19",
    "chromatic": "^4.0.3",
    "cypress": "^4.9.0",
    "cypress-axe": "^0.8.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.1",
    "husky": "^4.2.5",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1"
  },
  "optionalDependencies": {
    "babel-loader": "^8.1.0",
    "bufferutil": "^4.0.1",
    "canvas": "^2.6.1",
    "fibers": "^5.0.0",
    "react-is": "^16.13.1",
    "sass": "^1.26.7",
    "typescript": "^3.9.5",
    "utf-8-validate": "^5.0.2",
    "vue": "^2.6.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended",
      "plugin:chai-friendly/recommended"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/serviceWorker.js",
      "!src/index.js"
    ],
    "transform": {
      "^.+\\.[tj]sx?$": "babel-jest",
      "^.+\\.mdx$": "@storybook/addon-docs/jest-transform-mdx"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
